{
  // The name of the module
  "module": "DigitalClock",

  // Where to place it on screen
  "position": {
    "horizontalAlignment": "right",
    "verticalAlignment": "top",
    "margin": {
      "left": 0,
      "top": 0,
      "right": 5,
      "bottom": 0
    } 
  },
  // Attributes
  "attributes": {
    "timeFormat": "24hr",
    "dateFormat": "D",
    "dateCulture": "en-us"
  }
}